script.
      function initMap() {
        var latLng = {lat: 52.0577800, lng: 5.11108};
        var mapDiv = document.getElementById('map');
        var map = new google.maps.Map(mapDiv, {
            center: latLng,
            zoom: 16,
            scrollwheel: false,
        });

        var marker = new google.maps.Marker({
            position: latLng,
            map: map
        });

        var infowindow = new google.maps.InfoWindow({
            //content:"<h1 style='color:black;'>Codestar</h1>"
            content:"<div><img width='200' src='logo-zwart-notext.png'/></div>"
            //content:"<div style='background-color:red'><img width='200' src='logo-simple.svg'/></div>"
        })

        infowindow.open(map, marker)

        map.addListener('tilesloaded', function() {
            // Disable tabindex for all google map links
            var links = document.querySelectorAll('#map a');
            var i
            for (i = 0; i < links.length; ++i) {
                links[i].tabIndex = -1
            }
        })

        google.maps.event.addDomListener(window, 'resize', function() {
          map.setCenter(latLng)
        })
      }

      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-100358098-1', 'auto');
      ga('send', 'pageview');
            
script(src='https://maps.googleapis.com/maps/api/js?callback=initMap', async='', defer='')

